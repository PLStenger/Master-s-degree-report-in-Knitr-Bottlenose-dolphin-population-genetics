g <- g + geom_point(aes(colour = Selection), alpha = 0.8, size=3)
g <- g + theme(axis.text=element_text(size=18), axis.title=element_text(size=18), legend.key.size=unit(1,"cm"), legend.title=element_blank(), legend.text=element_text(size=18), legend.position="top")
g <- g +  xlab("Hétérozygotie") + ylab(expression(F[ST]))
